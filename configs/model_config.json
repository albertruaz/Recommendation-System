{
  "data_preprocessing": {
    "test_size": 0.2,
    "random_state": 42,
    "default_days": 30
  },
  "als": {
    "factors": 10,
    "regularization": 0.1,
    "iterations": 15,
    "alpha": 1.0,
    "use_gpu": false
  },
  "rating_rules": {
    "purchase": {
      "value": 5.0,
      "table": "purchased_product",
      "user_col": "customer_id",
      "item_col": "archived_id",
      "count_col": "purchase_count"
    },
    "cart": {
      "value": 4.5,
      "table": "action_log",
      "user_col": "member_id",
      "item_col": "product_id",
      "event_name": "CartItemPutEvent",
      "count_col": "cart_count"
    },
    "like": {
      "value": 4.0,
      "table": "action_log",
      "user_col": "member_id",
      "item_col": "product_id",
      "event_name": "LikedEvent",
      "count_col": "like_count"
    },
    "view": {
      "table": "product_view",
      "user_col": "member_id",
      "item_col": "product_id",
      "count_col": "view_count",
      "thresholds": [
        {
          "min_count": 3,
          "value": 3.8
        },
        {
          "min_count": 1,
          "value": 3.0
        }
      ]
    },
    "impression": {
      "table": "product_impression",
      "user_col": "member_id",
      "item_col": "product_id",
      "count_col": "impression_count",
      "thresholds": [
        {
          "min_count": 9,
          "value": -3.0
        },
        {
          "min_count": 6,
          "value": -2.0
        },
        {
          "min_count": 3,
          "value": -1.0
        }
      ]
    }
  },
  "filter_conditions": {
    "min_impression_count": 3,
    "require_any": ["view_count", "like_count", "cart_count", "purchase_count"]
  }
}
